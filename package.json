{
  "name": "coldcallcoach",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:frontend": "vite",
    "dev:backend": "tsx watch api/server.ts",
    "build": "tsc --project tsconfig.json && vite build",
    "vercel-build": "tsc --project tsconfig.json && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
